// This is your Prisma schema file,
// learn more about it in the docs: https://pris.ly/d/prisma-schema

generator client {
  provider = "prisma-client-js"
}

datasource db {
  provider = "sqlite"
  url      = env("DATABASE_URL")
}

model Auction {
	id Int @id @default(autoincrement())
  reservePrice Int
	seller String
	winner String?
	soldAmount Int
	currentHighBid Int
	createdAt String
	updatedAt String
	auctionEnd String
	status String
	make String
	model String
	year Int
	color String
	mileage Int
	imageUrl String
}
